body
  font-size 36px
  text-align center
  padding 0
  margin 0

  &:before
    content ''
    display block
    width 100%
    height 10000px
    background url("../favicon.png")
    opacity 0.05
    position absolute
    z-index 0
    top 0

button
  font-size 36px
  padding 10px
  cursor pointer
  position relative
  z-index 1